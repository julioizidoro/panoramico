	<queryString>
		<![CDATA[SELECT distinct dependente.nome as nomedependente, dependente.datanascimento, dependente.matricula, cliente.nome as nomecliente
from
	dependente join associado on dependente.associado_idassociado = associado.idassociado
    join cliente on associado.cliente_idcliente = cliente.idcliente
where dependente.datanascimento<=$P{datasql}
order by cliente.nome, dependente.nome]]>
	</queryString>
